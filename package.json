{
  "name": "vite-plugin-svg-icons-monorepo",
  "private": true,
  "version": "3.0.0-pre.1",
  "license": "MIT",
  "author": "Vben",
  "scripts": {
    "stub": "pnpm run prepack --filter ./packages -- --stub",
    "postinstall": "pnpm run stub",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"packages/**/*.{ts,tsx}\" --fix",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "test": "vitest"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "svg",
    "sprite",
    "svgo",
    "vben"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anncwb/vite-plugin-svg-icons"
  },
  "bugs": {
    "url": "https://github.com/anncwb/vite-plugin-svg-i cons/issues"
  },
  "homepage": "https://github.com/anncwb/vite-plugin-svg-icons/tree/master/#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-html": "^7.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vite": "^2.9.16",
    "vitest": "^0.32.2"
  },
  "pnpm": {
    "overrides": {
      "decode-uri-component": "^0.2.2",
      "loader-utils": "^2",
      "minimist": "^1.2.6",
      "postcss": ">=7.0.36",
      "uuid": "^7",
      "vite": ">2.9.16"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "packages/*/{src,types}/**/*.ts": [
      "eslint --ext .ts"
    ],
    "packages/**/*.d.ts": [
      "eslint --ext .ts"
    ]
  }
}
